<?xml version="1.1" encoding="UTF-8" standalone="no" ?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://local.xsd/dbchangelog-4.4.xsd"
        context="schema-change">

    <changeSet id="23-Add_identifier_type_to_identifier" context="schema-change" author="cgendreau">
        <addColumn tableName="identifier">
            <column name="identifier_type_id" type="integer">
                <constraints foreignKeyName="fk_identifier_to_identifier_type_id" references="identifier_type(id)" />
            </column>
        </addColumn>
    </changeSet>
</databaseChangeLog>
